<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="fill_parent"    android:background="#f3f3f3"    android:gravity="fill_vertical|center_horizontal"    android:orientation="vertical"    android:scrollbars="vertical" >    <LinearLayout        android:layout_width="fill_parent"        android:layout_height="0dp"        android:layout_gravity="fill"        android:layout_weight="1"        android:orientation="vertical" >    <include layout="@layout/stamp_barcode_title"         android:layout_width="fill_parent"        android:layout_height="wrap_content"/>        <ScrollView            android:id="@+id/scrollView1"            android:layout_width="match_parent"            android:layout_height="wrap_content" >            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content" >                <LinearLayout                    android:layout_width="fill_parent"                    android:layout_height="match_parent"                    android:orientation="vertical" >                    <ImageView                        android:id="@+id/curStampsImageView"                        android:layout_width="240dp"                        android:layout_height="40dp"                        android:layout_gravity="center_horizontal"                        android:layout_marginBottom="6.7dp"                        android:layout_marginTop="26dp"                        android:src="@drawable/coupon_title_stamp" />                    <ImageView                        android:id="@+id/curStampsNumImageView"                        android:layout_width="240dp"                        android:layout_height="93.3dp"                        android:layout_gravity="center_horizontal"                        android:layout_marginBottom="25.3dp"                        android:src="@drawable/coupon_card_a" />                    <ImageView                        android:id="@+id/curCardsImageView"                        android:layout_width="240dp"                        android:layout_height="40dp"                        android:layout_gravity="center_horizontal"                        android:layout_marginBottom="6.7dp"                        android:src="@drawable/coupon_title_avail" />                    <TableLayout                        android:id="@+id/stampCardTable"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_horizontal" >                        <TableRow                            android:id="@+id/stampCardBasicTableRow"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="center_horizontal" >                                                        <ImageView                                android:id="@+id/cardNullImageView"                                android:layout_width="240dp"                                android:layout_height="46.7dp"                                android:layout_gravity="center_horizontal"                                android:src="@drawable/coupon_cup_null"                                android:visibility="visible" />                        </TableRow>                    </TableLayout>                    <ImageView                        android:id="@+id/imageView1"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_horizontal"                        android:layout_marginTop="6dp"                        android:src="@drawable/coupon_announce" />                </LinearLayout>            </LinearLayout>        </ScrollView>    </LinearLayout>    <include        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_gravity="bottom"        layout="@layout/common_bottomtab" /></LinearLayout>